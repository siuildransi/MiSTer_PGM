<misterromdescription>
    <name>Demon Front (V105)</name>
    <mameversion>0245</mameversion>
    <setname>dmnfrnt</setname>
    <mratimestamp>20260211120000</mratimestamp>
    <year>2002</year>
    <manufacturer>IGS</manufacturer>
    <category>Run n Gun</category>
    <rbf>PGM</rbf>

    <!-- ============================================= -->
    <!-- index=0: Todo lo que va a SDRAM (DDRAM)       -->
    <!-- Se carga secuencialmente desde offset 0       -->
    <!-- ============================================= -->
    <rom index="0" zip="pgm.zip|dmnfrnt.zip" md5="None">
        <!-- 1. PGM BIOS (512KB) - desde pgm.zip -->
        <part name="pgm_p01s.u20"/>

        <!-- 2. Main CPU Program (P-ROM V105) - desde dmnfrnt.zip -->
        <part name="v105_16m.u5"/>

        <!-- 3. T-ROM (Tilemap/Text Graphics) -->
        <part name="pgm_t01s.rom"/>
        <part name="t04501.u29"/>

        <!-- 4. A-ROM (Sprite Pixel Data) -->
        <part name="a04501.u3"/>
        <part name="a04502.u4"/>
        <part name="a04503.u6"/>

        <!-- 5. B-ROM (Sprite Mask/Attributes) -->
        <part name="b04501.u9"/>
        <part name="b04502.u11"/>
    </rom>

    <!-- ============================================= -->
    <!-- index=1: Audio (Z80 ROM + Samples)            -->
    <!-- Va a Sound RAM vÃ­a dpram_dc                   -->
    <!-- ============================================= -->
    <rom index="1" zip="pgm.zip|dmnfrnt.zip" md5="None">
        <!-- Z80 Program ROM -->
        <part name="pgm_m01s.rom"/>

        <!-- ICS2115 Wavetable Samples -->
        <part name="w04501.u5"/>
    </rom>
</misterromdescription>
